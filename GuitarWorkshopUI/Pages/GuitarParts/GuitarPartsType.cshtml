@page
@model GuitarWorkshopUI.Pages.GuitarParts.GuitarPartsTypeModel
@{
    ViewData["Title"] = "Guitar Part Types";
}

<h2 class="text-center mb-4">Guitar Part Types</h2>

<div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
    <div class="col">
        <a asp-page="./BodyShape/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Body Shapes</a>
    </div>
    <div class="col">
        <a asp-page="./Finish/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Finishes</a>
    </div>
    <div class="col">
        <a asp-page="./FretNumber/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Fret Number Types</a>
    </div>
    <div class="col">
        <a asp-page="./GuitarColor/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Guitar Colors</a>
    </div>
    <div class="col">
        <a asp-page="./HeadstockStyle/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Headstock Styles</a>
    </div>
    <div class="col">
        <a asp-page="./NeckScale/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Neck Scales</a>
    </div>
    <div class="col">
        <a asp-page="./NeckShape/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Neck Shapes</a>
    </div>
    <div class="col">
        <a asp-page="./StringType/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">String Types</a>
    </div>
    <div class="col">
        <a asp-page="./TuningMachine/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Tuning Machines</a>
    </div>
    <div class="col">
        <a asp-page="./WoodsType/GetAll" class="btn btn-outline-primary w-100 py-3 shadow-sm">Woods Types</a>
    </div>
</div>